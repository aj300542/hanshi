[
  {"line": "庭坚叩头"},
  {"line": "比因南康签判李次山宣义舟行"},
  {"line": "奉书"},
  {"line": "并寄双井"},
  {"line": "计夏末乃得通彻耳"},
  {"line": "急足者伏奉三月六日手诲"},
  {"line": "审别来侍奉万福"},
  {"line": "何慰如之"},
  {"line": "惠寄鲍诗、《扬州集》"},
  {"line": "实副所望"},
  {"line": "广陵四达之冲"},
  {"line": "人事良可厌"},
  {"line": "又有送故迎新之劳"},
  {"line": "计得近文字之日极少"},
