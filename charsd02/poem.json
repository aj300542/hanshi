[
  {"line": "兮"},
  {"line": "驰椒丘且焉止息"},
  {"line": "进不入以离尤兮"},
  {"line": "退将复修吾初服"},
  {"line": "制芰荷以为衣兮"},
  {"line": "集芙蓉以为裳"},
  {"line": "不吾知其亦已兮"},
  {"line": "苟余情其信芳"},
  {"line": "高余冠之岌岌兮"},
  {"line": "长余佩之陆离"},
  {"line": "芳与泽其杂糅兮"},
  {"line": "唯昭质其犹未亏"},
  {"line": "忽反顾以游目兮"},
  {"line": "将往观乎四荒"},
  {"line": "佩缤纷其繁饰兮"},
  {"line": "芳菲菲其弥章"},
  {"line": "民生各有所乐兮"},
  {"line": "余独好修以为常"},
  {"line": "虽体解吾犹未变兮"},
  {"line": "岂余心之可惩"},
  {"line": "女嬃之婵媛兮"},
  {"line": "申申其詈予"},
  {"line": "曰鲧婞直以亡身兮"},
  {"line": "终然夭乎羽之野"},
  {"line": "汝何博謇而好修兮"},
  {"line": "纷独有此姱节"},
  {"line": "薋菉葹以盈室兮"},
  {"line": "判独离而不服"},
  {"line": "众不可户说兮"},
  {"line": "孰云察余之中情"},
  {"line": "世并举而好朋兮"},
  {"line": "夫何茕独而不予听"},
  {"line": "依前圣以节中兮"},
  {"line": "喟凭心而历兹"},
  {"line": "济沅湘以南征兮"},
  {"line": "就重华而敶词启九辩与九歌兮"},
  {"line": "夏康娱以自纵"},
  {"line": "不顾难以图后兮"},
  {"line": "五子用失乎家衖"},
  {"line": "羿淫游以佚畋兮"},
  {"line": "又好射夫封狐"},
  {"line": "固乱流其鲜终兮"},
  {"line": "浞又贪夫厥家"},
  {"line": "浇身被服强圉兮"},
  {"line": "纵欲而不忍"},
  {"line": "日康娱而自忘兮"},
  {"line": "厥首用夫颠陨"},
  {"line": "夏桀之常违兮"},
  {"line": "乃遂焉而逢殃"},
  {"line": "后辛之菹醢兮"},
  {"line": "殷宗用而不长"},
  {"line": "汤禹俨而祗敬兮"},
  {"line": "周论道而莫差"},
  {"line": "举贤才而授能兮"},
  {"line": "循绳墨而不颇"},
  {"line": "皇天无私阿兮"},
  {"line": "览民德焉错辅"},
  {"line": "夫维圣哲之"}
]
