[
  {"line": "驾卿盛秋之际"},
  {"line": "提兵按边"},
  {"line": "风霜已寒"},
  {"line": "征驭良苦"},
  {"line": "如是别有事宜"},
  {"line": "可密奏来"},
  {"line": "朝廷以淮西军叛之后"},
  {"line": "每加过虑"},
  {"line": "长江上流一带"},
  {"line": "缓急之际"},
  {"line": "全藉卿军照管"},
  {"line": "可更戒飭所留军马"},
  {"line": "训练整齐"},
  {"line": "常若寇至"},
  {"line": "蕲阳江州两处"},
  {"line": "水军亦宜遣发"},
  {"line": "以防意外"},
  {"line": "如卿体国"},
  {"line": "岂待多言"},
  {"line": "付岳飞"}
