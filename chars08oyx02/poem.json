[
  {"line": "脩启"},
  {"line": "脩以衰病余生"},
  {"line": "蒙上恩宽假"},
  {"line": "哀其恳至"},
  {"line": "俾遂归老"},
  {"line": "自杜门里巷"},
  {"line": "与世日疏"},
  {"line": "惟窃自念"},
  {"line": "幸得早从当世贤者之游"},
  {"line": "其于钦向德义"},
  {"line": "未始少忘于心耳"},
  {"line": "近张寺丞自洛来"},
  {"line": "出所惠书"},
  {"line": "其为感慰"},
  {"line": "何可胜言"},
  {"line": "因得仰诇起居"},
  {"line": "喜承宴处优闲"},
  {"line": "履况清福"},
  {"line": "春候暄和"},
  {"line": "更冀为时爱重"},
  {"line": "以副搢绅所以有望者"},
  {"line": "非独田亩垂尽之人区区也"},
  {"line": "不宣"},
  {"line": "脩再拜"},
  {"line": "端明侍读留台执事"}
]
