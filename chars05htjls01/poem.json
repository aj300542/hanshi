[
  {"line": "乐毅、乐闲当代名家"},
  {"line": "而笔法汪濊"},
  {"line": "鲁仲连大义慷慨"},
  {"line": "为人排难解纷"},
  {"line": "故笔端起发"},
  {"line": "高密相间"},
  {"line": "疏阔咸妙"},
  {"line": "唐雎周绍"},
  {"line": "虽非大成"},
  {"line": "亦闲雅多致"},
  {"line": "俨如威而不猛"},
  {"line": "李斯法逾程邈"},
  {"line": "可称作者"},
  {"line": "此数君子藏骨抱筋"},
  {"line": "含文包质"},
  {"line": "每于兰亭春昼"},
  {"line": "遂穷年由未入古人之室"},
  {"line": "书学岂易言哉"},
  {"line": "元祐元年冬日庭坚书"}
]
