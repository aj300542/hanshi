[
  {"line": "芾顿首"},
  {"line": "再拜"},
  {"line": "承清问属邑捕蝗"},
  {"line": "海浦方暑"},
  {"line": "恭惟劳神弊邑"},
  {"line": "上赖德庇"},
  {"line": "幸无蝗生"},
  {"line": "而雨沾足"},
  {"line": "必遂小丰"},
  {"line": "闻海境去弊境百里以上"},
  {"line": "曾有些小"},
  {"line": "今已静尽"},
  {"line": "亦恐民讹"},
  {"line": "不足信也"},
  {"line": "近有秋祭文上呈"},
  {"line": "可发笑"},
  {"line": "鲁君素谤芾者"},
  {"line": "与薛至亲"},
  {"line": "一体加毁"},
  {"line": "幸天恩旷荡尽赖恩庇及此"},
  {"line": "愧惕愧惕"},
  {"line": "芾惶恐"}
