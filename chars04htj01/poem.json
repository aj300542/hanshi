[
  {"line": "庭坚顿首"},
  {"line": "两辱垂顾"},
  {"line": "甚惠"},
  {"line": "放逐不齿"},
  {"line": "因废人事"},
  {"line": "不能奉诣"},
  {"line": "甚愧来辱之意"},
  {"line": "所须拙字"},
  {"line": "天凉意适"},
  {"line": "或能三二纸"},
  {"line": "门下生辄又取去"},
  {"line": "六十老人"},
  {"line": "五月挥汗"},
  {"line": "今实不能办此"},
  {"line": "想聪明可照察也"},
  {"line": "承晚凉遂行"},
  {"line": "千万珍爱"},
  {"line": "象江皆亲旧"},
  {"line": "但盛暑非近笔砚时"},
  {"line": "未能作书"},
  {"line": "见者为道此意"},
  {"line": "庭坚顿首齐君足下"}
