[
  {"line": "于天津兮"},
  {"line": "夕余至乎西极"},
  {"line": "凤皇翼其承旗兮"},
  {"line": "高翱翔之翼翼"},
  {"line": "忽吾行此流沙兮"},
  {"line": "遵赤水而容与"},
  {"line": "麾蛟龙使梁津兮"},
  {"line": "诏西皇使涉予"},
  {"line": "路修远以多艰兮"},
  {"line": "腾众车使径待"},
  {"line": "路不周以左转兮"},
  {"line": "指西海以为期"},
  {"line": "屯余车其千乘兮"},
  {"line": "齐玉轪而并驰"},
  {"line": "驾八龙之婉婉兮"},
  {"line": "载云旗之委蛇"},
  {"line": "抑志而弭节兮"},
  {"line": "神高驰之邈邈"},
  {"line": "奏九歌而舞韶兮"},
  {"line": "聊假日以偷乐"},
  {"line": "陟升皇之赫戏兮"},
  {"line": "忽临睨夫旧乡"},
  {"line": "仆夫悲余马怀兮"},
  {"line": "蜷局顾而不行"},
  {"line": "乱曰已矣哉"},
  {"line": "国无人兮莫我知兮"},
  {"line": "又何怀乎故都"},
  {"line": "既莫足与为美政兮"},
  {"line": "吾将从彭咸之所居"}
]
