[
  {"line": "赤壁赋"},
  {"line": "壬戌之秋"},
  {"line": "七月既望"},
  {"line": "苏子与客泛舟游于赤壁之下"},
  {"line": "清风徐来"},
  {"line": "水波不兴"},
  {"line": "举酒属客"},
  {"line": "诵明月之诗"},
  {"line": "歌窈窕之章"},
  {"line": "少焉"},
  {"line": "月出于东山之上"},
  {"line": "徘徊于斗牛之间"},
  {"line": "白露横江"},
  {"line": "水光接天"},
  {"line": "纵一苇之所如"},
  {"line": "凌万顷之茫然"},
  {"line": "浩浩乎如冯虚御风"},
  {"line": "而不知其所止飘飘乎如遗世独立"},
  {"line": "羽化而登仙"},
  {"line": "于是饮酒乐甚"},
  {"line": "扣舷而歌之"},
  {"line": "歌曰桂棹兮兰桨"},
  {"line": "击空明兮溯流光"},
  {"line": "渺渺兮予怀"},
  {"line": "望美人兮天一方"},
  {"line": "”客有吹洞箫者"},
  {"line": "倚歌而和之"},
  {"line": "其声呜呜然"},
  {"line": "如怨如慕"},
  {"line": "如泣如诉"},
  {"line": "余音袅袅"},
  {"line": "不绝如缕"},
  {"line": "舞幽壑之潜蛟"},
  {"line": "泣孤舟之嫠妇"},
  {"line": "苏子愀然"},
  {"line": "正襟危坐而问客曰何为其然也"},
  {"line": "”客曰月明星稀"},
  {"line": "乌鹊南飞"},
  {"line": "此非曹孟德之诗乎"},
  {"line": "西望夏口"},
  {"line": "东望武昌"},
  {"line": "山川相缪"},
  {"line": "郁乎苍苍"},
  {"line": "此非孟德之困于周郎者乎"},
  {"line": "方其破荆州"},
  {"line": "下江陵"},
  {"line": "顺流而东也"},
  {"line": "舳舻千里"},
  {"line": "旌旗蔽空"},
  {"line": "酾酒临江"},
  {"line": "横槊赋诗"},
  {"line": "固一世之雄也"},
  {"line": "而今安在哉"},
  {"line": "况吾与子渔樵于江渚之上"},
  {"line": "侣鱼虾而友麋鹿"},
  {"line": "驾一叶之扁舟"},
  {"line": "举匏樽以相属"},
  {"line": "寄蜉蝣于天地"},
  {"line": "渺沧海之一粟"},
  {"line": "哀吾生之须臾"},
  {"line": "羡长江之无穷"},
  {"line": "挟飞仙以遨游"},
  {"line": "抱明月而长终"},
  {"line": "知不可乎骤得"},
  {"line": "托遗响于悲风"},
  {"line": "”苏子曰客亦知夫水与月乎"},
  {"line": "逝者"}
]
