[
  {"line": "王右军论书"},
  {"line": "战国至秦百余年"},
  {"line": "世虽纷纭"},
  {"line": "而奇伟之士亦非秦即楚"},
  {"line": "故其字法若苏季子"},
  {"line": "如武人劲弩"},
  {"line": "一发百中"},
  {"line": "张仪如江涛遇风"},
  {"line": "狂澜欲绝"},
  {"line": "淳于髡如云中别鹤"},
  {"line": "戛然长鸣"},
  {"line": "春申君如虫食木"},
  {"line": "远而益坚"},
  {"line": "田婴颜斶"},
  {"line": "篆楷精到"},
  {"line": "如寒梅雪霁"},
  {"line": "别有清芬"},
  {"line": "陈轮庄章"},
  {"line": "如流泉入涧"},
  {"line": "潺湲可闻"},
  {"line": "苏代舌辩纵横"},
  {"line": "故运笔窈窕"},
  {"line": "乐毅乐闲"}
]
