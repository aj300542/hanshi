[
  {"line": "脩启"},
  {"line": "气候不常"},
  {"line": "承动履清安"},
  {"line": "辱简诲存问感愧"},
  {"line": "脩拙疾如故"},
  {"line": "然请外非为疾"},
  {"line": "亦与诸公求罢"},
  {"line": "而从容于进退者异也"},
  {"line": "谅非遂请不能已"},
  {"line": "然亦必易遂也"},
  {"line": "承见谕敢及之"},
  {"line": "脩顿首"},
  {"line": "元珍学士"},
  {"line": "子固伸意"}
