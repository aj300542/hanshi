[
  {"line": "近世诸贤书画不能俱传"},
  {"line": "李伯时亦善书"},
  {"line": "今独称其画"},
  {"line": "王黄华亦善画"},
  {"line": "今独传其书"},
  {"line": "盖以所长掩其所短故也"},
  {"line": "子昂学士书画俱到"},
  {"line": "他日必能俱传无疑"},
  {"line": "大德五年八月五日"},
  {"line": "困学民鲜于枢题子昂画东坡像及书二赤壁赋后"},
  {"line": "东坡二赋"},
  {"line": "松雪每要书之"},
  {"line": "敻出诸书之右"},
  {"line": "故深得晋人书法"},
  {"line": "晚年行笔圆熟"},
  {"line": "度越唐人"},
  {"line": "乃知早会用意之深如此"},
  {"line": "后学唐棣敬题"},
  {"line": "高宗以用武而中兴"},
  {"line": "既御六龙"},
  {"line": "居九重"},
  {"line": "日与文臣对"},
  {"line": "故万几之暇"},
  {"line": "无以寓罔极之思"},
  {"line": "因从瘦金书而复二王"},
  {"line": "可谓无耻于前哲"},
  {"line": "予昔随集贤公在京师"},
  {"line": "亲见书此赋何啻数十"},
  {"line": "其运笔如飞"},
  {"line": "于近代宗室中尤觉蕴藉"},
  {"line": "画则李龙眠学顾虎头"},
  {"line": "公继龙眠者也"},
  {"line": "临卷敬莫"},
  {"line": "大梁后学班惟志再拜书时至顺二年中秋日"},
  {"line": "至正丁酉岁拜观先人平章手书东坡前后赤壁赋"},
  {"line": "并画东坡小像"},
  {"line": "真迹无疑"},
  {"line": "藏者宜宝之"},
  {"line": "男奕谨识"}
]
