[
  {"line": "峻茂兮"},
  {"line": "愿俟时乎吾将刈"},
  {"line": "虽萎绝其亦何伤兮"},
  {"line": "哀众芳之芜秽"},
  {"line": "众皆竞进以贪婪兮"},
  {"line": "凭不厌乎求索"},
  {"line": "羌内恕己以量人兮"},
  {"line": "各兴心而嫉妒"},
  {"line": "忽驰骛以追逐兮"},
  {"line": "非余心之所急"},
  {"line": "老冉冉其将至兮"},
  {"line": "恐修名之不立"},
  {"line": "朝饮木兰之坠露兮"},
  {"line": "夕餐秋菊之落英"},
  {"line": "苟余情其信姱以练要兮"},
  {"line": "长顑颔亦何伤"},
  {"line": "掔木根以结茞兮"},
  {"line": "贯薜荔之落蕊"},
  {"line": "矫菌桂以纫蕙兮"},
  {"line": "索胡绳之纚纚"},
  {"line": "謇吾法夫前修兮"},
  {"line": "非世俗之所服"},
  {"line": "虽不周于今之人兮"},
  {"line": "愿依彭咸之遗则"},
  {"line": "长太息以掩涕兮"},
  {"line": "哀民生之多艰"},
  {"line": "余虽好修姱以鞿羁兮"},
  {"line": "謇朝谇而夕替"},
  {"line": "既替余以蕙纕兮"},
  {"line": "又申之以揽茝"},
  {"line": "亦余心之所善兮"},
  {"line": "虽九死其犹未悔"},
  {"line": "怨灵修之浩荡兮"},
  {"line": "终不察夫民心"},
  {"line": "众女嫉余之蛾眉兮"},
  {"line": "谣诼谓余以善淫"},
  {"line": "固时俗之工巧兮"},
  {"line": "偭规矩而改错"},
  {"line": "背绳墨以追曲兮"},
  {"line": "竞周容以为度"},
  {"line": "忳郁邑余侘傺兮"},
  {"line": "吾独穷困乎此时也"},
  {"line": "宁溘死以流亡兮"},
  {"line": "余不忍为此态也"},
  {"line": "鸷鸟之不群兮"},
  {"line": "自前世而固然"},
  {"line": "何方圜之能周兮"},
  {"line": "夫孰异道而相安"},
  {"line": "屈心而抑志兮"},
  {"line": "忍尤而攘诟"},
  {"line": "伏清白以死直兮"},
  {"line": "固前圣之所厚"},
  {"line": "悔相道之不察兮"},
  {"line": "延伫乎吾将反"},
  {"line": "回朕车以复路兮"},
  {"line": "及行迷之未远"},
  {"line": "步余马于兰皋"}
]
