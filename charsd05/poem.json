[
  {"line": "之所同"},
  {"line": "汤禹俨而求合兮"},
  {"line": "挚咎繇而能调"},
  {"line": "苟中情其好修兮"},
  {"line": "又何必用夫行媒"},
  {"line": "说操筑于傅岩兮"},
  {"line": "武丁用而不疑"},
  {"line": "吕望之鼓刀兮"},
  {"line": "遭周文而得举"},
  {"line": "宁戚之讴歌兮"},
  {"line": "齐桓闻以该辅"},
  {"line": "及年岁之未晏兮"},
  {"line": "时亦犹其未央"},
  {"line": "恐鹈鴃之先鸣兮"},
  {"line": "使夫百草为之不芳"},
  {"line": "何琼佩之偃蹇兮"},
  {"line": "众薆然而蔽之"},
  {"line": "惟此党人之不谅兮"},
  {"line": "恐嫉妒而折之"},
  {"line": "时缤纷其变易兮"},
  {"line": "又何可以淹留"},
  {"line": "兰芷变而不芳兮"},
  {"line": "荃蕙化而为茅"},
  {"line": "何昔日之芳草兮"},
  {"line": "今直为此萧艾也"},
  {"line": "岂其有他故兮"},
  {"line": "莫好修之害也"},
  {"line": "余以兰为可恃兮"},
  {"line": "羌无实而容长"},
  {"line": "委厥美以从俗兮"},
  {"line": "苟得列乎众芳"},
  {"line": "椒专佞以慢慆兮"},
  {"line": "樧又欲充夫佩帏"},
  {"line": "既干进而务入兮"},
  {"line": "又何芳之能祗"},
  {"line": "固时俗之流从兮"},
  {"line": "又孰能无变化"},
  {"line": "览椒兰其若兹兮"},
  {"line": "又况揭车与江离"},
  {"line": "惟兹佩之可贵兮"},
  {"line": "委厥美而历兹"},
  {"line": "芳菲菲而难亏兮"},
  {"line": "芬至今犹未沬"},
  {"line": "和调度以自娱兮"},
  {"line": "聊浮游而求女"},
  {"line": "及余饰之方壮兮"},
  {"line": "周流观乎上下"},
  {"line": "灵氛既告余以吉占兮"},
  {"line": "历吉日乎吾将行"},
  {"line": "折琼枝以为羞兮"},
  {"line": "精琼爢以为粻"},
  {"line": "为余驾飞龙兮"},
  {"line": "杂瑶象以为车"},
  {"line": "何离心之可同兮"},
  {"line": "吾将远逝以自疏"},
  {"line": "邅吾道夫昆仑兮"},
  {"line": "路修远以周流"},
  {"line": "扬云霓之晻蔼兮"},
  {"line": "鸣玉鸾之啾啾"},
  {"line": "朝发轫"}
]
