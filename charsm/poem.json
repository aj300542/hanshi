[
  {"line": "大道久不下"},
  {"line": "与先未然耶"},
  {"line": "梁武评书至右军"},
  {"line": "谓龙跳天门"},
  {"line": "虎卧凤阁"},
  {"line": "此帖是已"},
  {"line": "诸家刻中皆未之有"},
  {"line": "世间神物岂默有靳惜者不欲使滥传耶"},
  {"line": "将好事犹未至也"},
  {"line": "有能砻片石刻以传远"},
  {"line": "仆愿供摹搨之役属"},
  {"line": "奔走南北"},
  {"line": "此时殆废"},
  {"line": "不知何时果此缘也"},
  {"line": "至元丁亥九月七日孟秋"},
  {"line": "书家化境难窥测"},
  {"line": "天际行云峡注泉"},
  {"line": "想见彩毫飞舞处"},
  {"line": "心忘大道手通玄"},
  {"line": "天启癸亥菊月十二日朱之蕃题"},
  {"line": "右军大道帖十字"},
  {"line": "松雪跋"},
  {"line": "鬱冈斋刻者是也"},
  {"line": "麓村云米南宫笔"},
  {"line": "具眼哉"}
]
