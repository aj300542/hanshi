[
  {"line": "驾一叶之扁舟"},
  {"line": "举匏樽以相属"},
  {"line": "寄蜉蝣于天地"},
  {"line": "渺沧海之一粟"},
  {"line": "哀吾生之须臾"},
  {"line": "羡长江之无穷"},
  {"line": "挟飞仙以遨游"},
  {"line": "抱明月而长终"},
  {"line": "知不可乎骤得"},
  {"line": "托遗响于悲风"},
  {"line": "苏子曰客亦知夫水与月乎"},
  {"line": "逝者如斯"},
  {"line": "而未尝往也盈虚者如彼"},
  {"line": "而卒莫消长也"},
  {"line": "盖将自其变者而观之"},
  {"line": "则天地曾不能以一瞬自其不变者而观之"},
  {"line": "则物与我皆无尽也"},
  {"line": "而又何羡乎"},
  {"line": "且夫天地之间"},
  {"line": "物各有主"},
  {"line": "苟非吾之所有"},
  {"line": "虽一毫而莫取"},
  {"line": "惟江上之清风"},
  {"line": "与山间之明月"},
  {"line": "耳得之而为声"},
  {"line": "目遇之而成色"},
  {"line": "取之无禁"},
  {"line": "用之不竭"},
  {"line": "是造物者之无尽藏也"},
  {"line": "而吾与子之所共食"},
  {"line": "客喜而笑"},
  {"line": "洗盏更平酌"},
  {"line": "肴核既尽"},
  {"line": "杯盘狼藉"},
  {"line": "相与枕藉乎舟中"},
  {"line": "不知东方之既白"},
  {"line": "轼去年作此赋"},
  {"line": "未尝轻出以示人"},
  {"line": "见者盖一二人而已"},
  {"line": "钦之有使至"},
  {"line": "求近文"},
  {"line": "遂亲书以寄"},
  {"line": "多难畏事"},
  {"line": "钦之爱我"},
  {"line": "必深藏之不出也"},
  {"line": "又有后赤壁赋"},
  {"line": "笔倦未能写"},
  {"line": "当俟后信"},
  {"line": "轼白"}
]
