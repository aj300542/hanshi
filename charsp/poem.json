[
  {"line": "五月四日"},
  {"line": "芾启"},
  {"line": "蒙书为尉"},
  {"line": "审道味清适"},
  {"line": "涟"},
  {"line": "陋邦也"},
  {"line": "林君必能言之"},
  {"line": "他至此见"},
  {"line": "未有所止"},
  {"line": "蹄涔不能容吞舟"},
  {"line": "闽士泛海"},
  {"line": "客游甚众"},
  {"line": "求门馆者常十辈"},
  {"line": "寺院下满"},
  {"line": "林亦在寺也"},
  {"line": "莱去海出陆有十程"},
  {"line": "已贻书应求"},
  {"line": "倘能具事"},
  {"line": "力至海乃可"},
  {"line": "此一舟至海三日尔"},
  {"line": "海蝗云自山东来"},
  {"line": "在弊邑境未过来尔"},
  {"line": "御寇所居"},
  {"line": "国不足"},
  {"line": "岂贤者欲去之兆乎"},
  {"line": "呵呵"},
  {"line": "甘贫乐淡乃士常事"},
  {"line": "一动未可知"},
  {"line": "宜审决去就也"},
  {"line": "便中奉状"},
  {"line": "芾顿首"},
  {"line": "葛君德忱阁下"}
]
