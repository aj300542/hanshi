[
  {"line": "洛神赋并序"},
  {"line": "黄初三年"},
  {"line": "余朝京师"},
  {"line": "还济洛川"},
  {"line": "古人有言"},
  {"line": "斯水之神"},
  {"line": "名曰宓妃"},
  {"line": "感宋玉对楚王说神女之事"},
  {"line": "遂作斯赋"},
  {"line": "其词曰"},
  {"line": "余从京域"},
  {"line": "言归东藩"},
  {"line": "背伊阙"},
  {"line": "越轘辕"},
  {"line": "经通谷"},
  {"line": "陵景山"},
  {"line": "日既西倾"},
  {"line": "车殆马烦"},
  {"line": "尔乃税驾乎蘅皋"},
  {"line": "秣驷乎芝田"},
  {"line": "容与乎阳林"},
  {"line": "流眄乎洛川"},
  {"line": "于是精移神骇"},
  {"line": "忽焉思散"},
  {"line": "俯则末察"},
  {"line": "仰以殊观"},
  {"line": "睹一丽人"},
  {"line": "于岩之畔"},
  {"line": "尔乃援御者而告之曰"},
  {"line": "尔有觌于彼者乎"},
  {"line": "彼何人斯"},
  {"line": "若此之艳也"},
  {"line": "御者对曰臣闻河洛之神"},
  {"line": "名曰宓妃"},
  {"line": "则君王之所见"},
  {"line": "也无乃是乎"},
  {"line": "其状若何"},
  {"line": "臣愿闻之"},
  {"line": "余告之曰其形也"},
  {"line": "翩若惊鸿"},
  {"line": "婉若游龙"},
  {"line": "荣曜秋菊"},
  {"line": "华茂春松"},
  {"line": "仿佛兮若轻云之蔽月"},
  {"line": "飘飖兮若流风之回雪"},
  {"line": "远而望之"},
  {"line": "皎若太阳升朝霞迫而察之"},
  {"line": "灼若芙蕖出渌波"},
  {"line": "纤纤得衷"},
  {"line": "修短合度"},
  {"line": "肩若削成"},
  {"line": "腰如约素"},
  {"line": "延颈秀项"},
  {"line": "皓质呈露"},
  {"line": "芳泽无加"},
  {"line": "铅华弗御"},
  {"line": "云髻峨峨"},
  {"line": "修眉联娟"},
  {"line": "丹唇外朗"},
  {"line": "皓齿内鲜"},
  {"line": "明眸善睐"},
  {"line": "靥辅承权"},
  {"line": "瓌姿艳逸"},
  {"line": "仪静体闲"},
  {"line": "柔情绰态"},
  {"line": "媚于语言"},
  {"line": "奇服旷世"},
  {"line": "骨像应图"},
  {"line": "披罗衣之璀粲兮"},
  {"line": "珥瑶碧之华琚"},
  {"line": "戴金翠之首饰"},
  {"line": "缀明珠以耀躯"},
  {"line": "践远游之文履"},
  {"line": "曳雾绡之轻裾"},
  {"line": "微幽兰之芳蔼兮"},
  {"line": "步踟蹰于山隅"},
  {"line": "于是忽焉纵体"},
  {"line": "以遨以嬉"},
  {"line": "左倚采旄"},
  {"line": "右荫桂旗攘"},
  {"line": "皓腕于神浒兮"},
  {"line": "采湍濑之玄芝"},
  {"line": "余情悦其淑美兮"},
  {"line": "心振荡而不怡"},
  {"line": "无良媒以接欢兮"},
  {"line": "托微波而通辞"},
  {"line": "愿诚素之先达兮"},
  {"line": "解玉佩以要之"},
  {"line": "嗟佳人之信修"},
  {"line": "羌习礼而明诗"},
  {"line": "抗琼珶以和予兮"},
  {"line": "指潜渊而为期"},
  {"line": "执眷眷之款实兮"},
  {"line": "惧斯灵之我欺"},
  {"line": "感交甫之弃言兮"},
  {"line": "怅犹豫而狐疑"},
  {"line": "收和颜而静志兮"},
  {"line": "申礼防以自持"},
  {"line": "于是洛灵感焉"},
  {"line": "徙倚彷徨"},
  {"line": "神光离合"},
  {"line": "乍阴乍阳"},
  {"line": "竦轻躯以鹤立"},
  {"line": "若将飞而未翔"},
  {"line": "践椒涂之郁烈"},
  {"line": "步蘅薄而流芳"},
  {"line": "超长吟以永慕兮"},
  {"line": "声哀厉而弥长"},
  {"line": "尔乃众灵杂遝"},
  {"line": "命俦啸侣"},
  {"line": "或戏清流"},
  {"line": "或翔神渚"},
  {"line": "或采明珠"},
  {"line": "或拾翠羽"},
  {"line": "从南湘之二妃"},
  {"line": "携汉滨之游女"},
  {"line": "叹匏瓜之无匹"},
  {"line": "咏牵牛之独处"},
  {"line": "扬轻袿之猗靡"},
  {"line": "翳修袖以延伫"},
  {"line": "体迅飞凫"},
  {"line": "飘忽若神"},
  {"line": "陵波微步"},
  {"line": "罗袜生尘"},
  {"line": "动无常则"},
  {"line": "若危若安"},
  {"line": "进止难期"},
  {"line": "若往若还"},
  {"line": "转眄流精"},
  {"line": "光润玉颜"},
  {"line": "含辞未吐"},
  {"line": "气若幽兰"},
  {"line": "华容婀娜"},
  {"line": "令我忘餐"},
  {"line": "于是屏翳收风"},
  {"line": "川后静波"},
  {"line": "冯夷鸣鼓"},
  {"line": "女娲清歌"},
  {"line": "腾文鱼以警乘"},
  {"line": "鸣玉鸾以偕逝"},
  {"line": "六龙俨其齐首"},
  {"line": "载云车之容裔"},
  {"line": "鲸鲵踊而夹毂"},
  {"line": "水禽翔而为卫"},
  {"line": "于是越北沚"},
  {"line": "过南冈"},
  {"line": "纡素领"},
  {"line": "回清阳"},
  {"line": "动朱唇以徐言"},
  {"line": "陈交接之大纲"},
  {"line": "恨人神之道殊"},
  {"line": "怨盛年之莫当"},
  {"line": "抗罗袂以掩涕兮"},
  {"line": "泪流襟之浪浪"},
  {"line": "悼良会之永绝兮"},
  {"line": "哀一逝而异乡"},
  {"line": "无微情以效爱兮"},
  {"line": "献江南之明珰"},
  {"line": "虽潜处于太阴"},
  {"line": "长寄心于君王"},
  {"line": "忽不悟其所舍"},
  {"line": "怅神宵而蔽光"},
  {"line": "于是背下陵高"},
  {"line": "足往神留"},
  {"line": "遗情想像"},
  {"line": "顾望怀愁"},
  {"line": "冀灵体之复形"},
  {"line": "御轻舟而上溯"},
  {"line": "浮长川而忘返"},
  {"line": "思绵绵而增欷"},
  {"line": "夜耿耿而不寐"},
  {"line": "沾繁霜而至曙"},
  {"line": "命仆夫而就驾"},
  {"line": "吾将归乎东路"},
  {"line": "揽騑辔以抗荣"},
  {"line": "怅盘桓而不能去"},
  {"line": "子昂"}
]
