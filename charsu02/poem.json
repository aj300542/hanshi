[
  {"line": "后赤壁赋"},
  {"line": "是岁十月之望"},
  {"line": "步自雪堂"},
  {"line": "将归于临皋"},
  {"line": "二客从余"},
  {"line": "过黄泥之坂"},
  {"line": "霜露既降"},
  {"line": "木叶尽脱"},
  {"line": "人影在地"},
  {"line": "仰见明月"},
  {"line": "顾而乐之"},
  {"line": "行歌相答"},
  {"line": "已而叹曰有客无酒"},
  {"line": "有酒无肴"},
  {"line": "月白风清"},
  {"line": "如此良夜何"},
  {"line": "客曰今者薄暮"},
  {"line": "举网得鱼"},
  {"line": "巨口细鳞"},
  {"line": "状如松江之鲈"},
  {"line": "顾安所得酒乎"},
  {"line": "归而谋诸妇"},
  {"line": "妇曰我有斗酒藏之久矣"},
  {"line": "以待子不时之需"},
  {"line": "于是携酒与鱼复游于赤壁之下"},
  {"line": "江流有声"},
  {"line": "断岸千尺"},
  {"line": "山高月小"},
  {"line": "水落石出"},
  {"line": "曾日月之几何"},
  {"line": "而江山不可复识矣"},
  {"line": "余乃摄衣而上"},
  {"line": "履巉岩"},
  {"line": "披蒙茸"},
  {"line": "踞虎豹"},
  {"line": "登虬龙"},
  {"line": "攀栖鹘之危巢"},
  {"line": "俯冯夷之幽宫"},
  {"line": "盖二客不能从焉"},
  {"line": "划然长啸"},
  {"line": "草木震动"},
  {"line": "山鸣谷应"},
  {"line": "风起水涌"},
  {"line": "余亦悄然而悲"},
  {"line": "肃然而恐"},
  {"line": "凛乎其不可留也"},
  {"line": "返而登舟"},
  {"line": "放乎中流"},
  {"line": "听其所止而休焉"},
  {"line": "时夜将半"},
  {"line": "四顾寂寥"},
  {"line": "适有孤鹤"},
  {"line": "横江东来"},
  {"line": "翅如车轮"},
  {"line": "玄裳缟衣"},
  {"line": "戛然长鸣"},
  {"line": "掠余舟而西也"},
  {"line": "须臾客去"},
  {"line": "予亦就睡"},
  {"line": "梦二道士"},
  {"line": "羽衣蹁跹"},
  {"line": "过临皋之下"},
  {"line": "揖余而言曰赤壁之游"},
  {"line": "乐乎"},
  {"line": "”问其姓名"},
  {"line": "俯而不答"},
  {"line": "呜呼噫嘻"},
  {"line": "我知之矣"},
  {"line": "畴昔之夜"},
  {"line": "飞鸣而过我者"},
  {"line": "非子也耶"},
  {"line": "道士顾笑"},
  {"line": "余亦惊寤"},
  {"line": "开户视之"},
  {"line": "不见其处"},
  {"line": "子昂为岳上人书"},
  {"line": "大德三年十一月晦"}
]
