[
  {"line": "繣其难迁"},
  {"line": "夕归次于穷石兮"},
  {"line": "朝濯发乎洧盘"},
  {"line": "保厥美以骄傲兮"},
  {"line": "日康娱以淫游"},
  {"line": "虽信美而无礼兮"},
  {"line": "来违弃而改求"},
  {"line": "览相观于四极兮"},
  {"line": "周流乎天余乃下"},
  {"line": "望瑶台之偃蹇兮"},
  {"line": "见有娀之佚女"},
  {"line": "吾令鸩为媒兮"},
  {"line": "鸩告余以不好"},
  {"line": "雄鸠之鸣逝兮"},
  {"line": "余犹恶其佻巧"},
  {"line": "心犹豫而狐疑兮"},
  {"line": "欲自适而不可"},
  {"line": "凤皇既受诒兮"},
  {"line": "恐高辛之先我"},
  {"line": "欲远集而无所止兮"},
  {"line": "聊浮游以逍遥"},
  {"line": "及少康之未家兮"},
  {"line": "留有虞之二姚"},
  {"line": "理弱而媒拙兮"},
  {"line": "恐导言之不固"},
  {"line": "世溷浊而嫉贤兮"},
  {"line": "好蔽美而称恶"},
  {"line": "闺中既以邃远兮"},
  {"line": "哲王又不寤"},
  {"line": "怀朕情而不发兮"},
  {"line": "余焉能忍而与此终古"},
  {"line": "索琼茅以筳篿兮"},
  {"line": "命灵氛为余占之"},
  {"line": "曰两美其必合兮"},
  {"line": "孰信修而慕之"},
  {"line": "思九州之博大兮"},
  {"line": "岂惟是其有女"},
  {"line": "曰勉远逝而无狐疑兮"},
  {"line": "孰求美而释女"},
  {"line": "何所独无芳草兮"},
  {"line": "尔何怀乎故宇"},
  {"line": "世幽昧以昡曜兮"},
  {"line": "孰云察余之善恶"},
  {"line": "民好恶其不同兮"},
  {"line": "惟此党人其独异"},
  {"line": "户服艾以盈要兮"},
  {"line": "谓幽兰其不可佩"},
  {"line": "览察草木其犹未得兮"},
  {"line": "岂珵美之能当"},
  {"line": "苏粪壤以充祎兮"},
  {"line": "谓申椒其不芳"},
  {"line": "欲从灵氛之吉占兮"},
  {"line": "心犹豫而狐疑"},
  {"line": "巫咸将夕降兮"},
  {"line": "怀椒糈而要之"},
  {"line": "百神翳其备降兮"},
  {"line": "九疑缤其并迎"},
  {"line": "皇剡剡其扬灵兮"},
  {"line": "告余以吉故"},
  {"line": "曰勉升降以上下兮"},
  {"line": "求矩矱"}
]
