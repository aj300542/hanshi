[
  {"line": "芾顿首"},
  {"line": "再启"},
  {"line": "弊居在丹徒行衙之西"},
  {"line": "翛闲堂"},
  {"line": "漾月佳丽亭在其后"},
  {"line": "临运河之阔水"},
  {"line": "东则月台"},
  {"line": "西乃西山"},
  {"line": "故宝晋斋之西为致爽轩"},
  {"line": "环居桐柳椿杉百十本"},
  {"line": "以药植之"},
  {"line": "今十年皆垂荫一亩"},
  {"line": "真一亩之居也"},
  {"line": "四月末"},
  {"line": "上皇山樵人"},
  {"line": "以异石告"},
  {"line": "遂视之"},
  {"line": "八十一穴"},
  {"line": "大如碗"},
  {"line": "小容指"},
  {"line": "制在淮山一品之上"},
  {"line": "百夫运致宝晋桐杉之间"},
  {"line": "五月望"},
  {"line": "甘露满石次"},
  {"line": "林木焦苇"},
  {"line": "莫不沾"},
  {"line": "洁白如玉珠"},
  {"line": "郡中图去"},
  {"line": "至今未止"},
  {"line": "云欲上既不请亦不止也"},
  {"line": "芾顿首再拜"}
]
