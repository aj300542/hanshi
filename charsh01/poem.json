[
  {"line": "书之为艺"},
  {"line": "非他艺比也"},
  {"line": "历世圣贤重之"},
  {"line": "盖宣人文"},
  {"line": "施治化"},
  {"line": "述六经"},
  {"line": "应万事"},
  {"line": "经天纬地"},
  {"line": "不能外此"},
  {"line": "至百千万年日用而不可阙者"},
  {"line": "而岂他技艺之能比哉"},
  {"line": "是故自庖牺作书契"},
  {"line": "三皇五帝"},
  {"line": "无不通于书者"},
  {"line": "世远不能得见其迹"},
  {"line": "古文尚书与世传石刻"},
  {"line": "亦彷佛其万乙"},
  {"line": "皆非谬恶者矣"},
  {"line": "周人善篆书"},
  {"line": "孔子为人作题匾大字"},
  {"line": "观阙里床前硑"},
  {"line": "延陵季子墓石可见已"},
  {"line": "汉晋唐宋帝王皆学书"},
  {"line": "汉光武章帝晋武帝元帝唐太宗宋太宗高宗"},
  {"line": "其尤也"},
  {"line": "当时臣庶多能书者杜度张芝钟元常王逸少欧虞褚薛颜柳苏黄米蔡"},
  {"line": "其尤也"},
  {"line": "其悉工于书者"},
  {"line": "岂图为人观美哉"},
  {"line": "盖天之文与地之文人之文一也"},
  {"line": "景星庆云"},
  {"line": "人皆仰之精金美玉"},
  {"line": "人皆宝之"},
  {"line": "谬恶之书"},
  {"line": "见者乱吐"},
  {"line": "覆缶且不暇"},
  {"line": "何以垂世传远哉"},
  {"line": "余尝患世之不能书者"},
  {"line": "不自咎其拙恶"},
  {"line": "而以书为末技借口也"},
  {"line": "苏文忠公大节表者"},
  {"line": "文章妙天下"},
  {"line": "其书师颜鲁公"},
  {"line": "规模淳厚"},
  {"line": "筋骨隐映"},
  {"line": "古意浑成"},
  {"line": "中藏至巧"},
  {"line": "如周鼎秦钟"},
  {"line": "使人可爱"},
  {"line": "固可以破愚起懦于千百载之下"},
  {"line": "今赵君景辉"},
  {"line": "既耽学古人书"},
  {"line": "于文忠公此纸"},
  {"line": "玩之不啻共璧"},
  {"line": "书之所进"},
  {"line": "他日安知不与古人并传哉"},
  {"line": "余姑识此为之兆云"},
  {"line": "永乐元年癸未九月十一日"},
  {"line": "翰林学士庐陵解缙书"}
]
