[
  {"line": "轼将渡海"},
  {"line": "宿澄迈"},
  {"line": "承令子见访"},
  {"line": "知从者未归"},
  {"line": "又云恐已到桂府"},
  {"line": "若果尔"},
  {"line": "庶几得于海康相遇"},
  {"line": "不尔"},
  {"line": "则未知后会之期也"},
  {"line": "区区无他祷"},
  {"line": "惟晚景宜倍万自爱耳"},
  {"line": "匆匆留此纸"},
  {"line": "令子处更不重封"},
  {"line": "不罪不罪"},
  {"line": "轼顿首"},
  {"line": "梦得秘校阁下"},
  {"line": "六月十三日"},
  {"line": "手启梦得秘校"}
]
