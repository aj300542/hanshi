[
  {"line": "闻张都大宣德"},
  {"line": "权提举榆柳局在𣏌者"},
  {"line": "倘蒙明公荐此职"},
  {"line": "为成此河事"},
  {"line": "致薄效何如"},
  {"line": "芾再拜"},
  {"line": "南京以上"},
  {"line": "曲多未尝浅"},
  {"line": "又以明曲"},
  {"line": "则水逶迤"},
  {"line": "又自来南京以上"},
  {"line": "方有水头"},
  {"line": "以曲折乃能到"},
  {"line": "向下则无水头"},
  {"line": "此理是否"}
]
