[
  {"line": "青天有月来几时"},
  {"line": "我今停杯一问之"},
  {"line": "人攀明月不可得"},
  {"line": "月行却与人相随"},
  {"line": "皎如飞镜临丹阙"},
  {"line": "绿烟灭尽清辉发"},
  {"line": "但见宵从海上来"},
  {"line": "宁知晓向云间没"},
  {"line": "白兔捣药秋复春"},
  {"line": "嫦娥孤栖与谁邻"},
  {"line": "今人不见古时月"},
  {"line": "今月曾经照古今人"},
  {"line": "古人今人若流水"},
  {"line": "共看明月皆如此"},
  {"line": "惟愿当歌对酒时"},
  {"line": "月光长照金樽里"}
]
