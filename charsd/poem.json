[
  {"line": "离骚经\n帝高阳之苗裔兮"},
  {"line": "朕皇考曰伯庸"},
  {"line": "摄提贞于孟陬兮"},
  {"line": "惟庚寅吾以降"},
  {"line": "皇览揆余(于)初度兮"},
  {"line": "肇锡余以嘉名"},
  {"line": "名余曰正则兮"},
  {"line": "字余曰灵均"},
  {"line": "纷吾既有此内美兮"},
  {"line": "又重之以修能"},
  {"line": "扈江离与辟芷兮"},
  {"line": "纫秋兰以为佩"},
  {"line": "汩余若将不及兮"},
  {"line": "恐年岁之不吾与"},
  {"line": "朝搴阰之木兰兮"},
  {"line": "夕揽洲之宿莽"},
  {"line": "日月忽其不淹兮"},
  {"line": "春与秋其代序"},
  {"line": "惟草木之零落兮"},
  {"line": "恐美人之迟暮"},
  {"line": "不抚壮而弃秽兮"},
  {"line": "何不改(乎)此度"},
  {"line": "乘骐骥以驰骋兮"},
  {"line": "来吾道夫先路"},
  {"line": "昔三后之纯粹兮"},
  {"line": "固众芳之所在"},
  {"line": "杂申椒与菌桂兮"},
  {"line": "岂惟纫夫蕙茝"},
  {"line": "彼尧舜之耿介兮"},
  {"line": "既遵道而得路"},
  {"line": "何桀纣之猖披兮"},
  {"line": "夫唯捷径以窘步"},
  {"line": "惟夫党人之偷乐兮"},
  {"line": "路幽昧以险隘"},
  {"line": "岂余身之惮殃兮"},
  {"line": "恐皇舆之败绩"},
  {"line": "忽奔走以先后兮"},
  {"line": "及前王之踵武"},
  {"line": "荃不查余之中情兮"},
  {"line": "反信谗而齌怒"},
  {"line": "余固知謇謇之为患兮"},
  {"line": "忍而不能舍也"},
  {"line": "指九天以为正兮"},
  {"line": "夫唯灵修之故也"},
  {"line": "曰黄昏以为期兮"},
  {"line": "羌中道而改路"},
  {"line": "初既与余成言兮"},
  {"line": "后悔遁而有他"},
  {"line": "余既不难夫离别兮"},
  {"line": "伤灵修之数化"},
  {"line": "余既滋兰之九畹兮"},
  {"line": "又树蕙之百亩"},
  {"line": "畦留夷与揭车兮"},
  {"line": "杂杜衡与芳芷"},
  {"line": "冀枝叶之"}
]
