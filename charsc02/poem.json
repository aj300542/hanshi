[
  {"line": "虬龙"},
  {"line": "攀栖鹘之危巢"},
  {"line": "俯冯夷之幽宫"},
  {"line": "盖二客不能从焉"},
  {"line": "划然长啸"},
  {"line": "草木震动"},
  {"line": "山鸣谷应"},
  {"line": "风起水涌"},
  {"line": "予亦悄然而悲"},
  {"line": "肃然而恐"},
  {"line": "凛乎其不可留也"},
  {"line": "反而登舟"},
  {"line": "放乎中流"},
  {"line": "听其所止而休焉"},
  {"line": "时夜将半"},
  {"line": "四顾寂寥"},
  {"line": "适有孤鹤"},
  {"line": "横江东来"},
  {"line": "翅如车轮"},
  {"line": "玄裳缟衣"},
  {"line": "戛然长鸣"},
  {"line": "掠予舟而西也"},
  {"line": "须臾客去"},
  {"line": "予亦就睡"},
  {"line": "梦二道士"},
  {"line": "羽衣蹁跹"},
  {"line": "过临皋之下"},
  {"line": "揖予而言曰赤壁之游乐乎"},
  {"line": "问其姓名"},
  {"line": "俯而不答"},
  {"line": "呜呼"},
  {"line": "噫嘻"},
  {"line": "我知之矣"},
  {"line": "畴昔之夜"},
  {"line": "飞鸣而过我者"},
  {"line": "非子也邪"},
  {"line": "道士顾笑"},
  {"line": "予亦惊寤"},
  {"line": "开户视之"},
  {"line": "不见其处"},
  {"line": "大德辛丑正月八日"},
  {"line": "明远弟以此纸求书二赋"},
  {"line": "为书于松雪斋"},
  {"line": "并作东坡像于卷首"},
  {"line": "子昂"}
]
