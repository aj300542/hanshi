[
  {"line": "婴香"},
  {"line": "角沉三两末之"},
  {"line": "丁香四钱末之"},
  {"line": "龙脑七钱别研"},
  {"line": "麝香三钱别研"},
  {"line": "治弓甲香一钱两末之"},
  {"line": "右都研匀"},
  {"line": "入牙艳消半两"},
  {"line": "再研匀"},
  {"line": "入炼蜜六两"},
  {"line": "和匀"},
  {"line": "荫一月取出"},
  {"line": "丸作鸡头大"},
  {"line": "略记得如此"},
  {"line": "俟检得册子"},
