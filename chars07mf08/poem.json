[
  {"line": "戏成呈"},
  {"line": "司谏台坐"},
  {"line": "芾"},
  {"line": "我思岳麓抱黄阁"},
  {"line": "飞泉元在半天落"},
  {"line": "石鲸吐水"},
  {"line": "出湔一里"},
  {"line": "赤日雾起阴纷薄"},
  {"line": "我曾坐石浸足眠"},
  {"line": "时项抵水洗背肩"},
  {"line": "客时效我病欲死"},
  {"line": "一夜转筋著艾燃"},
  {"line": "关漼"},
  {"line": "如今病渴拥炉坐"},
  {"line": "安得缩却三十年"},
  {"line": "重往坐石浸足眠"},
  {"line": "呜呼"},
  {"line": "安得缩却三十年"}
]
