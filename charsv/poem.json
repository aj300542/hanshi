[
  {"line": "七月六日"},
  {"line": "熹顿首"},
  {"line": "前日一再附问"},
  {"line": "想无不达"},
  {"line": "便至承书"},
  {"line": "喜闻比日所履佳胜"},
  {"line": "小一嫂千一哥以次俱安"},
  {"line": "老拙衰病"},
  {"line": "幸未即死但脾胃终是怯弱"},
  {"line": "饮食小失节"},
  {"line": "便觉不快"},
  {"line": "兼作脾泄挠人"},
  {"line": "目疾则尤害事"},
  {"line": "更看文字不得也"},
  {"line": "吾弟虽亦有此疾"},
  {"line": "然来书尚能作小字"},
  {"line": "则亦未及此之什一也"},
  {"line": "千一哥且喜向安"},
  {"line": "若更要药"},
  {"line": "可见报"},
  {"line": "当附去"},
  {"line": "吕集卷秩甚多"},
  {"line": "曾道夫寄来者"},
  {"line": "尚未得看"},
  {"line": "续当寄出"},
  {"line": "不知子澄家上下百卷者是何本也"},
  {"line": "子约想时相见"},
  {"line": "曾无疑书已到未"},
  {"line": "如未到"},
  {"line": "别写去也"},
  {"line": "叶尉便中复附此"},
  {"line": "草草"},
  {"line": "余惟自爱之祝"},
  {"line": "不宣"},
  {"line": "熹顿首"},
  {"line": "允夫纠掾贤弟"}
]
