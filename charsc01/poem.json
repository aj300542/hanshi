[
  {"line": "如斯"},
  {"line": "而未尝往也盈虚者如彼"},
  {"line": "而卒莫消长也"},
  {"line": "盖将自其变者而观之"},
  {"line": "则天地曾不能以一瞬自其不变者而观之"},
  {"line": "则物与我皆无尽也"},
  {"line": "而又何羡乎且夫天地之间"},
  {"line": "物各有主苟非吾之所有"},
  {"line": "虽一毫而莫取"},
  {"line": "惟江上之清风"},
  {"line": "与山间之明月"},
  {"line": "耳得之而为声"},
  {"line": "目遇之而成色"},
  {"line": "取之无禁"},
  {"line": "用之不竭"},
  {"line": "是造物者之无尽藏也"},
  {"line": "而吾与子之所共适"},
  {"line": "客喜而笑"},
  {"line": "洗盏更酌"},
  {"line": "肴核既尽"},
  {"line": "杯盘狼籍"},
  {"line": "相与枕藉乎舟中"},
  {"line": "不知东方之既白"},
  {"line": "后赤壁赋"},
  {"line": "是岁十月之望"},
  {"line": "步自雪堂"},
  {"line": "将归于临皋"},
  {"line": "二客从予过黄泥之坂"},
  {"line": "霜露既降"},
  {"line": "木叶尽脱"},
  {"line": "人影在地"},
  {"line": "仰见明月"},
  {"line": "顾而乐之"},
  {"line": "行歌相答"},
  {"line": "已而叹曰有客无酒"},
  {"line": "有酒无肴"},
  {"line": "月白风清"},
  {"line": "如此良夜何"},
  {"line": "客曰今者薄暮"},
  {"line": "举网得鱼"},
  {"line": "巨口细鳞"},
  {"line": "状如松江之鲈"},
  {"line": "顾安所得酒乎"},
  {"line": "归而谋诸妇"},
  {"line": "妇曰我有斗酒"},
  {"line": "藏之久矣"},
  {"line": "以待子不时之需"},
  {"line": "于是携酒与鱼"},
  {"line": "复游于赤壁之下"},
  {"line": "江流有声"},
  {"line": "断岸千尺山高月小"},
  {"line": "水落石出"},
  {"line": "曾日月之几何"},
  {"line": "而江山不可复识矣"},
  {"line": "予乃摄衣而上"},
  {"line": "履谗岩"},
  {"line": "披蒙茸"},
  {"line": "踞虎豹"},
  {"line": "登"}
]
