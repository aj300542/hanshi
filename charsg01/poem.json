[
  {"line": "履道醇固"},
  {"line": "智足以任天下之重"},
  {"line": "学足以知先王之言"},
  {"line": "逮事厚陵"},
  {"line": "遍仪侍从之列"},
  {"line": "被遇文考"},
  {"line": "擢总枢机之繁"},
  {"line": "有大臣特立之风"},
  {"line": "蹈君子难进之节"},
  {"line": "方予访落之始"},
  {"line": "起应秉钧之求"},
  {"line": "调娱万几"},
  {"line": "必先教化之意"},
  {"line": "辨察百职"},
  {"line": "不失礼义之中"},
  {"line": "是用咨诹僉言"},
  {"line": "褒加异数"},
  {"line": "越升左相之路"},
  {"line": "兼峻东台之班"},
  {"line": "申衍爰田"},
  {"line": "陪敦真食"},
  {"line": "於戏"},
  {"line": "上寅亮于天心"},
  {"line": "则阴阳风雨以之顺"},
  {"line": "下遂字乎物理"},
  {"line": "则山川草木以之宁"},
  {"line": "内阜安于兆民"},
  {"line": "外镇抚于四裔"},
  {"line": "盖辅相者"}
]
