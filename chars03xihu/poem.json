[
  {"line": "夜泛西湖"},
  {"line": "新月生魄迹未安"},
  {"line": "才破五六渐盘桓"},
  {"line": "今夜吐艳如半璧"},
  {"line": "游人得向三更看"},
  {"line": "三更向阑月渐垂"},
  {"line": "欲落未落景特奇"},
  {"line": "明朝人事谁料得"},
  {"line": "看到苍龙西起时"},
  {"line": "苍龙已起斗牛横"},
  {"line": "东方芒角升长庚"},
  {"line": "渔人收筒及未晓"},
  {"line": "船过惟有菰蒲声"},
  {"line": "菰蒲无边水茫茫"},
  {"line": "荷花夜开风露香"},
  {"line": "渐见灯明出远寺"},
  {"line": "更待月黑看湖光"},
  {"line": "湖光非鬼亦非仙"},
  {"line": "风恬浪静光满川"},
  {"line": "须臾两两入寺去"},
  {"line": "就视不见空茫然"},
  {"line": "湖上初晴后雨"},
  {"line": "朝曦迎客艳重冈"},
  {"line": "晚雨留人入醉乡"},
  {"line": "此意自佳君不会"},
  {"line": "一杯当属水仙王"},
  {"line": "湖上有水仙王庙"},
  {"line": "水光潋滟晴方"},
  {"line": "山色空蒙雨亦奇"},
  {"line": "若把西湖比西子"},
  {"line": "淡妆浓抹总相宜"},
  {"line": "好"},
  {"line": "寒食未明至湖上太守未来两县令先在"},
  {"line": "城头月落尚啼乌"},
  {"line": "乌榜红舷早满湖"},
  {"line": "鼓吹未容迎五马"},
  {"line": "水云先已飏双凫"},
  {"line": "映山黄帽螭头舫"},
  {"line": "夹道青烟鹊尾炉"},
  {"line": "老病逢春只思睡"},
  {"line": "独求僧榻寄须臾"},
  {"line": "延祐七年秋"},
  {"line": "与明远游西湖诸名刹"},
  {"line": "见东坡数作"},
