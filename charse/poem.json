[
  {"line": "摩诃般若波罗蜜多心经"},
  {"line": "观自在菩萨"},
  {"line": "行深般若波罗蜜多时"},
  {"line": "照见五蕴皆空"},
  {"line": "度一切苦厄"},
  {"line": "舍利子"},
  {"line": "色不异空"},
  {"line": "空不异色"},
  {"line": "色即是空"},
  {"line": "空即是色"},
  {"line": "受想行识亦复如是"},
  {"line": "舍利子"},
  {"line": "是诸法空相"},
  {"line": "不生不灭"},
  {"line": "不垢不净"},
  {"line": "不增不减"},
  {"line": "是故空中无色"},
  {"line": "无受想行识"},
  {"line": "无眼耳鼻舌身意"},
  {"line": "无色声香味触法"},
  {"line": "无眼界乃至无意识界"},
  {"line": "无无明亦无无明尽"},
  {"line": "乃至无老死"},
  {"line": "亦无老死尽"},
  {"line": "无苦集灭道"},
  {"line": "无智亦无得"},
  {"line": "以无所得故"},
  {"line": "菩提萨埵"},
  {"line": "依般若波罗蜜多故"},
  {"line": "心无挂碍无挂碍故"},
  {"line": "无有恐怖"},
  {"line": "远离颠倒梦想"},
  {"line": "究竟涅槃"},
  {"line": "三世诸佛"},
  {"line": "依般若波罗蜜多故"},
  {"line": "得阿耨多罗三藐三菩提"},
  {"line": "故知般若波罗蜜多"},
  {"line": "是大神咒"},
  {"line": "是大明咒"},
  {"line": "是无上咒"},
  {"line": "是无等等咒"},
  {"line": "能除一切苦"},
  {"line": "真实不虚"},
  {"line": "故说般若波罗蜜多咒"},
  {"line": "即说咒曰：揭谛揭谛"},
  {"line": "波罗揭谛"},
  {"line": "波罗僧揭谛"},
  {"line": "菩提萨婆诃"},
  {"line": "嘉靖二十一年岁"},
  {"line": "在壬寅九月廿又一日"},
  {"line": "书于昆山舟中"},
  {"line": "徵明"}
]
